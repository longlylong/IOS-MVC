source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '8.0'
use_frameworks!

target 'wysdkdemo' do

pod 'Bolts'

#支付的sdk
pod 'Pingpp/Alipay' #, :git => 'https://github.com/PingPlusPlus/pingpp-ios.git'
pod 'Pingpp/Wx' #, :git => 'https://github.com/PingPlusPlus/pingpp-ios.git'

#缓存库
pod 'Kingfisher'

#加密的
pod 'CryptoSwift'

#json
pod 'HandyJSON'
end
